
#
# Build configuration
#
CONFIG_BUILD_TYPE_W800S2=y
# CONFIG_BUILD_TYPE_W900 is not set
CONFIG_CHIP_NAME="W800S2"

#
# Image configuration
#
CONFIG_BUILD_IMAGE_HEADER=80D0000
CONFIG_BUILD_IMAGE_RUN_ADDRESS=80D0400
CONFIG_BUILD_IMAGE_UPDATE_ADDRESS=8010000
# end of Image configuration

#
# Bootloader configuration
#
CONFIG_BUILD_BOOTLOADER_HEADER=8002000
CONFIG_BUILD_BOOTLOADER_RUN_ADDRESS=8002400
CONFIG_BUILD_BOOTLOADER_UPDATE_ADDRESS=8010000
CONFIG_BUILD_BOOTLOADER_NEXT_HEADER=80D0000
# end of Bootloader configuration

CONFIG_BUILD_VERSION="G03.00.00"
# end of Build configuration

#
# Compiler configuration
#
CONFIG_TOOLCHAIN_PATH=""
CONFIG_TOOLCHAIN_PREFIX="csky-elfabiv2-"
# CONFIG_COMPILER_OPTIMIZE_LEVEL_OG is not set
CONFIG_COMPILER_OPTIMIZE_LEVEL_OS=y
# CONFIG_COMPILER_OPTIMIZE_LEVEL_O2 is not set
# CONFIG_COMPILER_OPTIMIZE_LEVEL_O0 is not set
CONFIG_COMPILER_OPTIMIZE_CCACHE=y
# end of Compiler configuration

#
# Partition Table
#
# CONFIG_WM_CHIP_FLASH_SIZE_1MB is not set
CONFIG_WM_CHIP_FLASH_SIZE_2MB=y
# CONFIG_WM_CHIP_FLASH_SIZE_4MB is not set
# CONFIG_WM_CHIP_FLASH_SIZE_8MB is not set
# CONFIG_WM_CHIP_FLASH_SIZE_16MB is not set
CONFIG_WM_PARTITION_TABLE_OFFSET=0x0
CONFIG_WM_PARTITION_TABLE_MD5=y
CONFIG_WM_PARTITION_TABLE_NORMAL_APP=y
# CONFIG_WM_PARTITION_TABLE_LARGE_APP is not set
# CONFIG_WM_PARTITION_TABLE_WITH_OTA is not set
# end of Partition Table

#
# Components configuration
#

#
# Driver
#

#
# UART configuration
#
CONFIG_DRIVER_UART_DEFAULT_TX_PIN_1=y
# CONFIG_DRIVER_UART_DEFAULT_TX_PIN_2 is not set
CONFIG_DRIVER_UART_DEFAULT_RX_PIN_1=y
# CONFIG_DRIVER_UART_DEFAULT_RX_PIN_2 is not set
# end of UART configuration

#
# SPI configuration
#
# end of SPI configuration
# end of Driver

#
# FreeRTOS
#
CONFIG_FREERTOS_HZ=500
CONFIG_FREERTOS_IDLE_TASK_STACKSIZE=256
CONFIG_FREERTOS_MAX_TASK_NAME_LEN=16
CONFIG_FREERTOS_USE_TRACE_FACILITY=y
CONFIG_FREERTOS_USE_STATS_FORMATTING_FUNCTIONS=y
CONFIG_FREERTOS_ENABLE_IDLE_HOOK=y
# CONFIG_FREERTOS_ENABLE_TICK_HOOK is not set
CONFIG_FREERTOS_QUEUE_REGISTRY_SIZE=0
CONFIG_FREERTOS_TIMER_TASK_PRIORITY=1
CONFIG_FREERTOS_TIMER_TASK_STACK_DEPTH=512
CONFIG_FREERTOS_TIMER_QUEUE_LENGTH=128
CONFIG_FREERTOS_ENABLE_STATIC_TASK_CLEAN_UP=y
CONFIG_FREERTOS_ASSERT_FAIL_ABORT=y
# CONFIG_FREERTOS_ASSERT_FAIL_PRINT_CONTINUE is not set
# CONFIG_FREERTOS_ASSERT_DISABLE is not set
CONFIG_FREERTOS_CHECK_STACKOVERFLOW=y
# end of FreeRTOS

#
# Heap
#
# CONFIG_HEAP_USE_PSRAM is not set
# CONFIG_HEAP_USE_TRACING is not set
CONFIG_HEAP_POISONING_DISABLED=y
# CONFIG_HEAP_POISONING_LIGHT is not set
# CONFIG_HEAP_POISONING_COMPREHENSIVE is not set
# end of Heap

#
# LWIP
#
CONFIG_LWIP_TASK_PRIO=11
CONFIG_LWIP_TASK_STACK_SIZE=4096
# CONFIG_LWIP_ENABLE_IPV6 is not set
# CONFIG_LWIP_ENABLE_NAPT is not set
# CONFIG_LWIP_ENABLE_DEBUG is not set
# end of LWIP

#
# Mbedtls
#
CONFIG_MBEDTLS_ENABLE_HARD_DRV=y
# CONFIG_MBEDTLS_ENABLE_DEBUG is not set
# end of Mbedtls

#
# Mqtt
#
CONFIG_MQTT_CLIENT_TASK_PRIO=4
CONFIG_MQTT_CLIENT_TASK_STACK_SIZE=2048
CONFIG_MQTT_WORK_TASK_PRIO=3
CONFIG_MQTT_WORK_TASK_STACK_SIZE=8192
# end of Mqtt

#
# Posix
#
CONFIG_COMPONENT_POSIX_ENABLED=y
CONFIG_POSIX_TASK_NAME_DEFAULT="pthread"
CONFIG_POSIX_TASK_PRIO_DEFAULT=0
CONFIG_POSIX_TASK_STACK_MIN=256
CONFIG_POSIX_TIMER_NAME_DEFAULT="timer"
CONFIG_POSIX_MQ_MAX_MESSAGES=10
CONFIG_POSIX_MQ_MAX_SIZE=128
# end of Posix

CONFIG_XT804_SEPARATE_IRQ_SP=y
CONFIG_XT804_ARCH_INTERRUPTSTACK=4096
CONFIG_XT804_IRQ_VECTOR_SIZE=256
CONFIG_XT804_SAVE_VR_REGISTERS=y
CONFIG_XT804_SAVE_HIGH_REGISTERS=y
CONFIG_XT804_SYSTEM_SECURE=y

#
# CLI
#
# CONFIG_COMPONENT_CLI_ENABLED is not set
# end of CLI

#
# Event
#
CONFIG_WM_EVENT_USE_TASK=y
CONFIG_WM_EVENT_TASK_PRIO=10
CONFIG_WM_EVENT_TASK_STACK_SIZE=2048
CONFIG_WM_EVENT_QUEUE_SIZE=4
# end of Event

#
# Log
#
# CONFIG_LOG_DEFAULT_LEVEL_NONE is not set
# CONFIG_LOG_DEFAULT_LEVEL_ERROR is not set
# CONFIG_LOG_DEFAULT_LEVEL_WARN is not set
CONFIG_LOG_DEFAULT_LEVEL_INFO=y
# CONFIG_LOG_DEFAULT_LEVEL_DEBUG is not set
# CONFIG_LOG_DEFAULT_LEVEL_VERBOSE is not set
CONFIG_LOG_DEFAULT_LEVEL=3
CONFIG_LOG_LINE_SIZE=1024
# CONFIG_LOG_USE_COLOR is not set
# CONFIG_LOG_USE_TASK is not set
# end of Log

#
# W800S2
#
CONFIG_W800S2_UART_PRINT_MODE_USE_UART0=y
# CONFIG_W800S2_UART_PRINT_MODE_USE_UART1 is not set
# CONFIG_W800S2_UART_PRINT_MODE_USE_NONE is not set
# end of W800S2

#
# WiFi
#
CONFIG_COMPONENT_WIFI_ENABLED=y

#
# MACSW configuration
#
CONFIG_WM_WL_ENABLE_11N=y
# CONFIG_WM_WL_ENABLE_AP is not set
# CONFIG_WM_WL_ENABLE_AP_11N is not set
# CONFIG_WM_WL_ENABLE_AP_PS is not set
# CONFIG_WM_WL_ENABLE_AP_FWD is not set
CONFIG_WM_WL_ENABLE_INFO_LOG=y
CONFIG_WM_WL_TX_BUF_NUM=7
CONFIG_WM_WL_RX_BUF_NUM=3
# CONFIG_WM_WL_ENABLE_DBG is not set
# end of MACSW configuration
# end of WiFi

#
# Wpa_supplicant
#
CONFIG_WPA_SUPPLICANT_TASK_PRIO=12
CONFIG_WPA_SUPPLICANT_TASK_STACK_SIZE=4096
# CONFIG_WPA_SUPPLICANT_ENABLE_DEBUG is not set
# CONFIG_WPA_SUPPLICANT_ENABLE_SET_MAC is not set
# end of Wpa_supplicant

#
# XT804
#
# end of XT804
# end of Components configuration
